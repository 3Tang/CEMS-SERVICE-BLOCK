public class SendBlockBean {
	private BlockBean blockBean;
	private List<DeviceBean> deviceBeans;
	
	
	public SendBlockBean(BlockBean blockBean, List<DeviceBean> deviceBeans) { 
		this.blockBean = blockBean;
		this.deviceBeans = deviceBeans;
	}
	public BlockBean getBlockBean() {
		return blockBean;
	}
	public void setBlockBean(BlockBean blockBean) {
		this.blockBean = blockBean;
	}
	public List<DeviceBean> getDeviceBeans() {
		return deviceBeans;
	}
	public void setDeviceBeans(List<DeviceBean> deviceBeans) {
		this.deviceBeans = deviceBeans;
	} 
}
 